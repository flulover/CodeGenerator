<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://namespace.zynx.com/2012/cdss/v1" xmlns:tns="http://namespace.zynx.com/2012/cdss/v1" targetNamespace="http://namespace.zynx.com/2012/cdss/v1" elementFormDefault="qualified" version="0.5.dev">
  <xs:element name="PerformanceMeasure" type="PerformanceMeasure"/>
  <xs:complexType name="PerformanceMeasure">
    <xs:annotation>
      <xs:documentation>Quality Measure impacted by the referencing item.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Id" type="xs:int">
        <xs:annotation>
          <xs:documentation>The Zynx performance measure ID.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GroupName" type="xs:string">
        <xs:annotation>
          <xs:documentation>The performance measure group name.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="Evidence" type="Evidence"/>
  <xs:complexType name="Evidence">
    <xs:annotation>
      <xs:documentation>Link to an evidence page</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Id" type="xs:string">
        <xs:annotation>
          <xs:documentation>The evidence page ID</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Link" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation>The URL reference to the evidence page.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Description" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A brief description of the evidence page.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Version" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The evidence page version.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IsZynxEvidence" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>When 'true', this evidence page is a Zynx evidence page.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:schema>
