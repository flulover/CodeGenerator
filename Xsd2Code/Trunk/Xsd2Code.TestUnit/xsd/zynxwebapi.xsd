<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://namespace.zynx.com/2012/cdss/v1" xmlns:tns="http://namespace.zynx.com/2012/cdss/v1" targetNamespace="http://namespace.zynx.com/2012/cdss/v1" elementFormDefault="qualified" version="0.5.dev">
	<xs:element name="AppliedDataTemplate" type="AppliedDataTemplate"/>
	<xs:complexType name="AppliedDataTemplate">
		<xs:complexContent>
			<xs:extension base="CdssEntity">
				<xs:sequence>
					<xs:element name="AppliedDataTemplateIdentifier" type="AppliedDataTemplateIdentifier"/>
					<xs:element name="DataTemplateReference" type="UnversionedPartitionedIdentifier"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="AppliedDataTemplateIdentifier" type="AppliedDataTemplateIdentifier"/>
	<xs:complexType name="AppliedDataTemplateIdentifier">
		<xs:sequence>
			<xs:element name="EnvironmentId" type="xs:int"/>
			<xs:element name="AppliesTo" type="xs:string"/>
			<xs:element name="Uri" type="xs:anyURI" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ArrayOfComponent" type="ArrayOfComponent"/>
	<xs:complexType name="ArrayOfComponent">
		<xs:sequence>
			<xs:element name="Component" type="Component" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ArrayOfCompositeOrderableGroupReference" type="ArrayOfCompositeOrderableGroupReference"/>
	<xs:complexType name="ArrayOfCompositeOrderableGroupReference">
		<xs:sequence>
			<xs:element name="CompositeOrderableGroupReference" type="UnversionedPartitionedIdentifier" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ArrayOfDataElementInstances" type="ArrayOfDataElementInstances"/>
	<xs:complexType name="ArrayOfDataElementInstances">
		<xs:sequence>
			<xs:element name="DataElementInstance" type="DataElementInstance" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The value for a specific attribute of a DataTemplate. For instance, the value for either a text field or drop down list in a data entry screen.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ArrayOfEnumerationReference" type="ArrayOfEnumerationReference"/>
	<xs:complexType name="ArrayOfEnumerationReference">
		<xs:sequence>
			<xs:element name="EnumerationReference" type="UnversionedPartitionedIdentifier" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ArrayOfErrorMessage" type="ArrayOfErrorMessage"/>
	<xs:complexType name="ArrayOfErrorMessage">
		<xs:sequence>
			<xs:element name="ErrorMessage" type="ErrorMessage" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ArrayOfEvidenceResources" type="ArrayOfEvidenceResources"/>
	<xs:complexType name="ArrayOfEvidenceResources">
		<xs:annotation>
			<xs:documentation>A collection of evidence resources.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Evidence" type="Evidence" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>An evidence link.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ArrayOfKcpOutcomes" type="ArrayOfKcpOutcomes"/>
	<xs:complexType name="ArrayOfKcpOutcomes">
		<xs:annotation>
			<xs:documentation>A collection of Key Clinical Processes</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="KcpOutcome" type="xs:string" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>An individual Key Clinical Process.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ArrayOfOrderedElement" type="ArrayOfOrderedElement"/>
	<xs:complexType name="ArrayOfOrderedElement">
		<xs:sequence>
			<xs:element name="OrderedElement" type="OrderedElement" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ArrayOfOrderSets" type="ArrayOfOrderSets"/>
	<xs:complexType name="ArrayOfOrderSets">
		<xs:annotation>
			<xs:documentation>Complex type representing a list of order set.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrderSet" type="OrderSet" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ArrayOfPerformanceMeasureResources" type="ArrayOfPerformanceMeasureResources"/>
	<xs:complexType name="ArrayOfPerformanceMeasureResources">
		<xs:annotation>
			<xs:documentation>A collection of performance measure resources.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PerformanceMeasure" type="PerformanceMeasure" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A performance measure.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ArrayOfSectionGroupReference" type="ArrayOfSectionGroupReference"/>
	<xs:complexType name="ArrayOfSectionGroupReference">
		<xs:sequence>
			<xs:element name="SectionGroupReference" type="UnversionedPartitionedIdentifier" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ArrayOfThirdPartyStandard" type="ArrayOfThirdPartyStandard"/>
	<xs:complexType name="ArrayOfThirdPartyStandard">
		<xs:sequence>
			<xs:element name="ThirdPartyStandard" type="ThirdPartyStandard" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="BooleanDataElementType" type="BooleanDataElementType"/>
	<xs:complexType name="BooleanDataElementType">
		<xs:sequence>
			<xs:element name="DefaultValue" type="xs:boolean" minOccurs="0"/>
			<xs:element name="Required" type="xs:boolean" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Bundle" type="Bundle"/>
	<xs:complexType name="Bundle">
		<xs:annotation>
			<xs:documentation>A collection of orderables that exhibit either an AND or OR behavior.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Metadata" type="ItemMetadata" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Metadata associated with a bundle such as edit comments, additional information, performance measures, evidence links, and key clinical processes.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Type" type="BundleBehavior">
				<xs:annotation>
					<xs:documentation>Specifies the bundle's section behavior. Bundle behaviors may be 'Group' or 'Selection'.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Preselect" type="PreselectEnumeration" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the selection behavior for the given item. Selection behavior may be one of the following: Optional, Default, DefaultWithReason, Required.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Components" type="CompositeOrderableComponents" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The components included in this bundle.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="BundleBehavior">
		<xs:annotation>
			<xs:documentation>Specifies the bundle's selection behavior. 'Group' specifies an AND selection behavior where either all items are selected, or none. 'Selection' specifies an OR section behavior where zero or more items in the bundle may be selected.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Group"/>
			<xs:enumeration value="Selection"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CatalogItemStatus">
		<xs:restriction base="xs:string">
			<xs:enumeration value="active"/>
			<xs:enumeration value="inactive"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="CatalogSummary" type="CatalogSummary"/>
	<xs:complexType name="CatalogSummary">
		<xs:sequence>
			<xs:element name="EnvironmentId" type="xs:int"/>
			<xs:element name="Uri" type="xs:anyURI"/>
			<xs:element name="IdspacesSummary" type="ResourceSummary"/>
			<xs:element name="SectionGroupsSummary" type="ResourceSummary"/>
			<xs:element name="SectionsSummary" type="ResourceSummary"/>
			<xs:element name="OrderableGroupsSummary" type="ResourceSummary"/>
			<xs:element name="OrderablesSummary" type="ResourceSummary"/>
			<xs:element name="CompositeOrderableGroupsSummary" type="ResourceSummary"/>
			<xs:element name="CompositeOrderablesSummary" type="ResourceSummary"/>
			<xs:element name="EnumerationsSummary" type="ResourceSummary"/>
			<xs:element name="EnumerationValuesSummary" type="ResourceSummary"/>
			<xs:element name="DataElementsSummary" type="ResourceSummary"/>
			<xs:element name="DataTemplatesSummary" type="ResourceSummary"/>
			<xs:element name="AppliedDataTemplatesSummary" type="ResourceSummary"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="CdssEntity" type="CdssEntity"/>
	<xs:complexType name="CdssEntity" abstract="true">
		<xs:sequence>
			<xs:element name="ErrorMessages" type="ArrayOfErrorMessage" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ClinicalConcept" type="ClinicalConcept"/>
	<xs:complexType name="ClinicalConcept">
		<xs:annotation>
			<xs:documentation>A Zynx-defined clinical concept along with a collection of semantically equivalent terms from standard terminologies and ontologies.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Type" type="CoverageType">
				<xs:annotation>
					<xs:documentation>The general category for the clinical concept.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ZynxConceptId" type="xs:string">
				<xs:annotation>
					<xs:documentation>The Zynx unique concept identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ZynxConceptDisplayName" type="xs:string">
				<xs:annotation>
					<xs:documentation>The Zynx concept display name for human consumption.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ZynxConceptDescription" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A human narrative description of the concept.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ThirdPartyStandards" type="ThirdPartyEncodings" minOccurs="0">
				<xs:annotation>
					<xs:documentation>References to semantically equivalent terms in standard terminologies</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ClinicalCoverage" type="ClinicalCoverage"/>
	<xs:complexType name="ClinicalCoverage">
		<xs:annotation>
			<xs:documentation>A collection of clinical concepts (generally based on standard terminologies) that can be associated with a knowledge artifact for information retrieval and categorization purposes.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Coverage" type="ClinicalConcept" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>An individual clinical or demographic concept of relevance.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="CodedConcept" type="CodedConcept"/>
	<xs:complexType name="CodedConcept">
		<xs:annotation>
			<xs:documentation>Represents a given term in a standard terminology or ontology such as SNOMED CT, RxNorm, etc...</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Code" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The code symbol for a given term or concept as defined in the terminology.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DisplayName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The human readable name for the given term/concept as it exists in the terminology.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CodeSystem" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The code system identifier for the code system that defines the code. This is typically an OID such as the following SNOMED CT OID: 2.16.840.1.113883.6.96</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CodeSystemDisplayName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The display name for the given code system such as "SNOMED CT"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CodeSystemVersion" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The version of the referenced code system when relevant.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ValueSet" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The value set OID or unique identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ValueSetVersion" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The version of the value set referenced by the coded concept, when relevant.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OriginalText" type="xs:string">
				<xs:annotation>
					<xs:documentation>The original text that this code is meant to codify and which represents the intended meaning of the user.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Type" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The type of code.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Component" type="Component"/>
	<xs:complexType name="Component">
		<xs:choice>
			<xs:element name="TextComponent" type="TextComponent" minOccurs="0"/>
			<xs:element name="OrderableComponent" type="OrderableComponent" minOccurs="0"/>
		</xs:choice>
	</xs:complexType>
	<xs:element name="ComponentDetail" type="ComponentDetail"/>
	<xs:complexType name="ComponentDetail">
		<xs:sequence>
			<xs:element name="Editable" type="xs:boolean"/>
			<xs:element name="DataTemplateReference" type="UnversionedPartitionedIdentifier" minOccurs="0"/>
			<xs:element name="DataElementValues" type="DataElementValues" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="CompositeOrderable" type="CompositeOrderable"/>
	<xs:complexType name="CompositeOrderable">
		<xs:complexContent>
			<xs:extension base="IdentifiableEntity">
				<xs:sequence>
					<xs:element name="Name" type="RequiredString1000"/>
					<xs:element name="Description" type="RequiredString1000"/>
					<xs:element name="Status" type="CatalogItemStatus"/>
					<xs:element name="PrimaryCompositeOrderableGroupReference" type="UnversionedPartitionedIdentifier"/>
					<xs:element name="SecondaryCompositeOrderableGroupReferences" type="ArrayOfCompositeOrderableGroupReference" minOccurs="0"/>
					<xs:element name="CompositeOrderableType" type="CompositeOrderableType"/>
					<xs:element name="OrderableComponentGroupingType" type="OrderableComponentGroupingType"/>
					<xs:element name="Components" type="ArrayOfComponent"/>
					<xs:element name="ThirdPartyStandards" type="ArrayOfThirdPartyStandard" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="CompositeOrderableComponents" type="CompositeOrderableComponents"/>
	<xs:complexType name="CompositeOrderableComponents">
		<xs:annotation>
			<xs:documentation>Possible choices for the components of Bundles and CompositeOrderables.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="ReminderComponent" type="TextItem">
					<xs:annotation>
						<xs:documentation>A clinical reminder.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OrderComponent" type="OrderableItem">
					<xs:annotation>
						<xs:documentation>An child component orderable.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TextComponent" type="TextItem">
					<xs:annotation>
						<xs:documentation>A textual, read-only, representation of an orderable.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="CompositeOrderableGroup" type="CompositeOrderableGroup"/>
	<xs:complexType name="CompositeOrderableGroup">
		<xs:complexContent>
			<xs:extension base="TermGroupBase">
				<xs:sequence>
					<xs:element name="ThirdPartyStandards" type="ArrayOfThirdPartyStandard" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="CompositeOrderableItem" type="CompositeOrderableItem"/>
	<xs:complexType name="CompositeOrderableItem">
		<xs:sequence>
			<xs:element name="CompositeOrderableReferences" type="ReferenceBindings">
				<xs:annotation>
					<xs:documentation>The reference to the composite orderable item in the Orderable Catalog.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Metadata" type="ItemMetadata" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Metadata associated with a composite orderable such as edit comments, name overrides, additional information, performance measures, evidence links, and key clinical processes.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Preselect" type="PreselectEnumeration" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the selection behavior for the given item. Selection behavior may be one of the following: Optional, Default, DefaultWithReason, Required.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CachedSentence" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Human-readable representation of this orderable item. This field is generally derived from the concatenation of the term display name or override, additional info, and the concatenated instance values of the AppliedDataTemplate.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AppliedData" type="DataTemplateInstance" minOccurs="0">
				<xs:annotation>
					<xs:documentation>AppliedData holds the instance values for user-defined metadata attributes common to all  composite orderable resources in a given global namespace. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Components" type="CompositeOrderableComponents" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The component orderables and reminders that make up this composite orderable.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="CompositeOrderableType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="iss"/>
			<xs:enumeration value="ivs"/>
			<xs:enumeration value="ost"/>
			<xs:enumeration value="pca"/>
			<xs:enumeration value="tpn"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CoverageType">
		<xs:annotation>
			<xs:documentation>The general concept category such as the gender of patient, the clinical focus of an artifact, or a workflow setting/task.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="PatientGender"/>
			<xs:enumeration value="PatientAgeGroup"/>
			<xs:enumeration value="ClinicalFocus"/>
			<xs:enumeration value="TargetUser"/>
			<xs:enumeration value="WorkflowSetting"/>
			<xs:enumeration value="WorkflowTask"/>
			<xs:enumeration value="ClinicalVenue"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="DataElement" type="DataElement"/>
	<xs:complexType name="DataElement">
		<xs:complexContent>
			<xs:extension base="IdentifiableEntity">
				<xs:sequence>
					<xs:element name="Name" type="RequiredString100"/>
					<xs:element name="Description" type="RequiredString255"/>
					<xs:element name="Status" type="CatalogItemStatus"/>
					<xs:element name="Label" type="RequiredString100"/>
					<xs:element name="IsInSentence" type="xs:boolean"/>
					<xs:element name="DataElementType" type="DataElementType"/>
					<xs:element name="ThirdPartyStandards" type="ArrayOfThirdPartyStandard" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="DataElementInstance" type="DataElementInstance"/>
	<xs:complexType name="DataElementInstance">
		<xs:annotation>
			<xs:documentation>The value for a specific attribute of a DataTemplate. For instance, the value for either a text field or drop down list in a data entry screen.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DataElementReference" type="UnversionedPartitionedIdentifier">
				<xs:annotation>
					<xs:documentation>A reference to a Data Element defined in an Orderable Catalog.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="FreeValue" type="xs:string">
					<xs:annotation>
						<xs:documentation>The textual value of a data element. Typically, the value entered in a text field.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="EnumerationValueReferences" type="ReferenceBindings">
					<xs:annotation>
						<xs:documentation>The reference to an enumeration value in an enumeration. For instance,  a reference to a particular selection in a drop down list such as 'Q8H' in a list of frequencies.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="DataElementType" type="DataElementType"/>
	<xs:complexType name="DataElementType">
		<xs:choice>
			<xs:element name="Boolean" type="BooleanDataElementType"/>
			<xs:element name="Text" type="TextDataElementType"/>
			<xs:element name="MultilineText" type="TextDataElementType"/>
			<xs:element name="SingleSelect" type="SingleSelectDataElementType"/>
			<xs:element name="Integer" type="IntegerDataElementType"/>
			<xs:element name="Decimal" type="DecimalDataElementType"/>
			<xs:element name="Hidden" type="xs:string"/>
		</xs:choice>
	</xs:complexType>
	<xs:element name="DataElementValue" type="DataElementValue"/>
	<xs:complexType name="DataElementValue">
		<xs:sequence>
			<xs:element name="DataElementReference" type="UnversionedPartitionedIdentifier"/>
			<xs:element name="DefaultValue" type="RequiredString255" minOccurs="0"/>
			<xs:element name="DefaultEnumerationValueReference" type="UnversionedPartitionedIdentifier" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="DataElementValues" type="DataElementValues"/>
	<xs:complexType name="DataElementValues">
		<xs:sequence>
			<xs:element name="DataElementValue" type="DataElementValue" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="DataTemplate" type="DataTemplate"/>
	<xs:complexType name="DataTemplate">
		<xs:complexContent>
			<xs:extension base="IdentifiableEntity">
				<xs:sequence>
					<xs:element name="Name" type="RequiredString100"/>
					<xs:element name="Description" type="RequiredString500"/>
					<xs:element name="Status" type="CatalogItemStatus"/>
					<xs:element name="OrderedElements" type="ArrayOfOrderedElement"/>
					<xs:element name="ThirdPartyStandards" type="ArrayOfThirdPartyStandard" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="DataTemplateInstance" type="DataTemplateInstance"/>
	<xs:complexType name="DataTemplateInstance">
		<xs:annotation>
			<xs:documentation>A DataTemplateInstance holds all attribute instance values for a given data template. For instance, a data template may specify a frequency attribute of type Enumeration with EnumerationValues such as 'Q8H', 'TID', 'Q4H', etc... A DataTemplateInstance for this DataTemplate may specify the value 'Q8H' for the frequency attribute.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DataTemplateReference" type="UnversionedPartitionedIdentifier">
				<xs:annotation>
					<xs:documentation>A reference to a DataTemplate that typically defines the set of fields that make up a 'data entry screen'.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DataElementInstances" type="ArrayOfDataElementInstances" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Free text values or selected list items (enumeration values) for each attribute defined in the referenced data template.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CachedSentence" type="xs:string" minOccurs="0">
				<xs:annotation>
